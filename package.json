{
  "name": "koinos-as-sdk-examples",
  "version": "0.3.0",
  "main": "assembly/index.ts",
  "license": "MIT",
  "author": "Roamin Roe <roamin@tuta.io>",
  "repository": "https://github.com/roaminroe/koinos-as-sdk-examples",
  "dependencies": {
    "@protobuf-ts/protoc": "^2.2.2",
    "commander": "^9.0.0",
    "koinos-as-sdk": "^0.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "as-proto-gen": "^0.1.0",
    "assemblyscript": "^0.19.22",
    "eslint": "^8.7.0",
    "koinos-abi-proto-gen": "^0.1.3",
    "koinos-as-gen": "^0.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "config": {
    "protocVersion": "3.4.0"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --ext ts \"*/assembly/**/*.ts\"",
    "build": "npm run generate:all && npm run build:debug",
    "upload": "node cli.js upload-contract ./calculator calculator debug",
    "generate:all": "npm run generate:abi && npm run generate:proto && npm run generate:as",
    "generate:abi": "node cli.js generate-abi ./calculator calculator",
    "generate:proto": "node cli.js generate-proto-files ./calculator",
    "generate:as": "node cli.js generate-as-files ./calculator calculator",
    "build:debug": "node cli.js build-as ./calculator debug"
  }
}
